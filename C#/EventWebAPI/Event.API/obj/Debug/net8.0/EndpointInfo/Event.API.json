  "components": {
    "schemas": {
      "UserInfo": {
        "required": [
          "emailId",
          "password",
          "role",
          "userName"
        ],
        "type": "object",
        "properties": {
          "emailId": {
            "minLength": 1,
            "type": "string"
          },
          "userName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "pattern": "Admin|Participant",
            "type": "string"
          },
          "password": {
            "maxLength": 20,
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
